programa {
  funcao inicio() {
    inteiro a,b
    escreva("Digite o primeiro numero\n")
    leia(a)
    escreva("Digite o segundo numero\n")
    leia(b)
    se(a > b){
      escreva("O primeiro numero é maior que o segundo")
    }senao se(b > a){
      escreva("O segundo numero é maior que o segundo")
    }senao{
      escreva("Os numeros são iguais")
    }
  }
}
